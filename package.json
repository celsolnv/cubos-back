  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node  ./node_modules/typeorm/cli -d ./src/infra/database/typeorm/typeorm.config.ts",
    "typeorm:migration-create": "typeorm migration:create ./src/infra/database/typeorm/migrations/$npm_config_name",
    "mg-run": "npm run typeorm migration:run",
    "mg-revert": "npm run typeorm migration:revert",
    "mg-create": "npm run typeorm:migration-create",
    "typeorm:migration-revet": "npm run typeorm migration:revert"
  },
  "dependencies": {
